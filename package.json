{
    "name": "spy-game-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@fontsource/roboto": "^4.5.8",
        "@mui/icons-material": "^5.11.0",
        "@mui/material": "^5.11.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "history": "^5.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.8.1",
        "react-scripts": "^5.0.1",
        "redux": "^4.2.1"
    },
    "scripts": {
        "setup": "npm run build; gnoDeploy setup spy-indieraydev-com spy-game dist 18.18.0",
        "deploy": "npm run build; gnoDeploy deploy spy-indieraydev-com spy-game dist 18.18.0",
        "start": "cp -a config.ui.development.js src/config.js; PORT=8080 react-scripts start",
        "serve": "PORT=3000 node srv/index.js",
        "config": "npm run config-ui; npm run config-app;",
        "config-app": "cp -a config.app.production.js srv/config.js; ",
        "config-ui": "cp -a config.ui.production.js src/config.js; ",
        "build": "npm run config; rm -r dist; mkdir dist/; npm run build-ui; npm run build-app; cp -a spy-indieraydev-com.* dist/;",
        "build-app": "cp -a srv/ dist/app/;",
        "build-ui": "react-scripts build; cp -a build/ dist/ui/;",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
